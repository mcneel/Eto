  </PropertyGroup>
  
  <ItemGroup>
    <ProjectReference Include="..\EtoApp.1\EtoApp.1.csproj" />
  </ItemGroup>
	
  <ItemGroup>
    <PackageReference Include="Eto.Platform.Windows" Version="2.5.11-dev" Condition="$(IncludeWinForms) == 'True'" />
    <PackageReference Include="Eto.Platform.Wpf" Version="2.5.11-dev" />
    <PackageReference Include="Eto.Platform.Gtk" Version="2.5.11-dev" />
    <PackageReference Include="Eto.Platform.Mac64" Version="2.5.11-dev" />
  </ItemGroup>
	
</Project>
